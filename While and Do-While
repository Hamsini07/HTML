<!DOCTYPE html>
<html>
<head>
    <title>Print 1 to 10 Using Loops</title>
    <script>
        function printNumbers() {
            let output = "";

            // Using for loop
            output += "For Loop: ";
            for (let i = 1; i <= 10; i++) {
                output += i + " ";
            }
            output += "\n";
            // Using while loop
            output += "While Loop: ";
            let j = 1;
            while (j <= 10) {
                output += j + " ";
                j++;
            }
            output += "\n";

            // Using do-while loop
            output += "Do-While Loop: ";
            let k = 1;
            do {
                output += k + " ";
                k++;
            } while (k <= 10);
            alert(output);
        }
    </script>
</head>
<body>
    <h2>Print Numbers 1 to 10 Using Loops</h2>
    <button onclick="printNumbers()">Show Numbers</button>
</body>
</html>
